#include "binary_trees.h"

/**
 *
 */
binary_tree_t *binary_tree_insert_right(binary_tree_t *parent, int value)
{
	binary_tree_t *temp, *new;

	if (parent == NULL)
		return (NULL);

	if (parent->left == NULL)
	{
		new = binary_tree_node(parent, value);
		parent->left = new;
	}
	else
	{
		temp = parent->left;
		new = binary_tree_node(parent, value);
		parent->left = new;
		temp->parent = new;
		new->left = temp;
	}

	return (new);
}
